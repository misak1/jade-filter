doctype html
html(lang='en')
  head
    title Jade
    //- link(rel="stylesheet" type="text/css" href="css/style.css")
    link(rel="stylesheet" href="./resources/css/index.css")
  body
    .main
      #content-header
        ul.header-menu.nav.nav-tabs
          // 一番最初に表示したいページに.is-current
          li.item.active(role='presentation')
            a(href='#', onclick='Load.layerPanel(0);') 設定
          li.item(role='presentation')
            a(href='#', onclick='Load.layerPanel(1);') 処理/実行
          li.item(role='presentation')
            a(href='#', onclick='Load.layerPanel(2);') HTMLダウンロード
          li.item(role='presentation')
            a(href='#', onclick='Load.layerPanel(3);') WEBスクレイピング
          li.item(role='presentation')
            a(href='#', onclick='Load.layerPanel(4);') データベース
          li.item(role='presentation')
            a(href='#', onclick='Load.layerPanel(5);') その他
      #content
        fieldset#LeftPanel
          #div_A.window.top
            .layer-panel
              .fileTree0
              // 設定
            .layer-panel
              .fileTree1
              // バッチ処理
            .layer-panel
              .fileTree2
              // HTMLダウンロード
            .layer-panel
              .fileTree3
              // WEBスクレイピング
            .layer-panel
              .fileTree4
              // データベース
            .layer-panel
              .fileTree5
              // その他
            .layer-panel none
          #div_left.handler_horizontal
        #div_vertical.handler_vertical
        fieldset#RightPanel
          #div_C.window.top
            .layer-panel
              // h3 設定
            .layer-panel
              // h3 バッチ処理
            .layer-panel
              // h3 HTMLダウンロード
            .layer-panel
              // h3 WEBスクレイピング
            .layer-panel
              // h3 データベース
            .layer-panel
              // h3 その他
          #div_right.handler_horizontal
      #content-footer
        .consolePanelMenu
          .btn-group-sm.console-btn-group(role='group', aria-label='...')
            a.u-mr10(onclick='Console.clear();') バッファクリア
            a.u-mr10(onclick='App.toggleDevTools();') Developer Tools
        
    :html
      <script src="//code.jquery.com/jquery-1.12.4.js"   integrity="sha256-Qw82+bXyGq6MydymqBxNPYTaUXXq7c8v3CwiYwLLNXU="   crossorigin="anonymous"></script>
      <script src="//code.jquery.com/ui/1.12.1/jquery-ui.js" integrity="sha256-T0Vest3yCU7pafRw9r+settMBX6JkKN06dqBnpQ8d30=" crossorigin="anonymous"></script>
      <script src="./resources/js/window/layout.js"></script>
  